# Agentic Memory Risk Service Environment Variables
RISK_SERVICE_PORT=3017
AGENTIC_MEMORY_ENABLED=true
AGENTIC_MEMORY_URL=http://localhost:3015
AGENTIC_MEMORY_TIMEOUT=5000

# Disable GP integration to use the range-based method
# The range-based method gave great performance before GPU training
ENABLE_GP_INTEGRATION=false

# Disable A/B testing since we want consistent range-based predictions
ENABLE_AB_TESTING=false

# Set to true for backtesting mode
BACKTEST_MODE=false